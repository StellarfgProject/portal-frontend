<?php

require_once('0.php');

$app = pg_fetch_assoc(pg_query_params($db, 'SELECT *, to_char(ts, \'YYYY-MM-DD HH:MI AM\') AS ts_formatted, to_char(claimed_at, \'YYYY-MM-DD HH:MI AM\') AS claimed_at_formatted, to_char(status_updated_at, \'YYYY-MM-DD HH:MI AM\') AS status_updated_at_formatted FROM app WHERE guid = $2 AND domain IN (SELECT domain FROM domain_access WHERE email = $1)', [$user, $_GET['guid']]));
// Fetch vehicle 2 data from the vehicle table
$vehicle2 = pg_fetch_assoc(pg_query_params($db, '
    SELECT v.* 
    FROM vehicle v 
    WHERE v.guid = $1', [$_GET['guid']])
);
$app['language'] = ($app['lang'] == 'es') ? 'Spanish' : 'English';
$app['finance_or_refinance'] = ($app['fi_or_refi'] == 'fi') ? 'Finance' : 'Refinance';

if (!$app) {
	audit('review', 'failure', isset($_GET['guid']) ? $_GET['guid'] : NULL);
    header('HTTP/1.0 401 Unauthorized');
    echo '<h1>Unauthorized</h1>';
    die();
} else {
	audit('review', 'single', $app['guid']);
}

$statuses = pg_fetch_all(pg_query_params($db, 'SELECT s.*, to_char(ts, \'YYYY-MM-DD HH:MI AM\') AS ts_formatted, h.first_name, h.last_name FROM status_log s LEFT JOIN human h ON s.email = h.email WHERE guid = $1 ORDER BY ts ASC', [$app['guid']]));

if ($app['claimed_by'] == '' && isset($_POST['claim'])) {
	pg_query_params($db, "UPDATE app SET claimed_by = $1, claimed_at = $2 WHERE guid = $3", [$user, 'now', $app['guid']]);
	audit('claim', 'single', $app['guid']);
	header("Location: {$_SERVER['REQUEST_URI']}");
	die();
}


if (isset($_POST['update_status']) && ($_POST['new_status'] != "")) {
	pg_query_params($db, "INSERT INTO status_log (ts, guid, status, status_note, email) values ($1, $2, $3, $4, $5)", ['now', $_GET['guid'], $_POST['new_status'], $_POST['new_status_note'], $user]);
	pg_query_params($db, "UPDATE app SET status = $1, status_note = $2, status_updated_at = $3 WHERE guid = $4", [$_POST['new_status'], $_POST['new_status_note'], 'now', $_GET['guid']]);
	header("Location: {$_SERVER['REQUEST_URI']}");
	die();
}

